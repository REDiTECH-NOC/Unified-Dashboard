# REDiTECH Relay Agent Configuration
# Copy to .env and fill in values

# URL of the REDiTECH Command Center app (no trailing slash)
RCC_API_URL=https://rcc.yourdomain.com

# API key from the "Register Agent" button in the 3CX dashboard
RCC_AGENT_API_KEY=rcc_ag_xxxxxxxxxxxxxxxxxxxxxxxx

# Poll interval in milliseconds (default: 5000 = 5 seconds)
POLL_INTERVAL_MS=5000
